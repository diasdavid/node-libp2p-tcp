{
  "name": "libp2p-tcp",
  "version": "0.17.2",
  "description": "Node.js implementation of the TCP module that libp2p uses, which implements the interface-connection and interface-transport interfaces",
  "main": "./dist/src/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js"
    }
  },
  "scripts": {
    "lint": "aegir ts -p check && aegir lint",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "aegir test -t node -f ./dist/test/*.js",
    "test:node": "aegir test -t node -f ./dist/test/*.js",
    "release": "aegir release -t node -f ./dist/test/*.js",
    "release-minor": "aegir release -t node -f ./dist/test/*.js --type minor",
    "release-major": "aegir-release -t node -f ./dist/test/*.js --type major",
    "coverage": "nyc --reporter=text --reporter=lcov npm run test:node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/libp2p/js-libp2p-tcp.git"
  },
  "keywords": [
    "libp2p",
    "network",
    "p2p",
    "peer",
    "peer-to-peer",
    "IPFS",
    "TCP"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/libp2p/js-libp2p-tcp/issues"
  },
  "homepage": "https://github.com/libp2p/js-libp2p-tcp",
  "engines": {
    "node": ">=14.0.0"
  },
  "types": "dist/src/index.d.ts",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^9.0.0",
    "aegir": "^36.0.0",
    "it-pipe": "^1.1.0",
    "libp2p-interfaces": "^3.1.1",
    "libp2p-interfaces-compliance-tests": "^3.1.1",
    "sinon": "^12.0.0",
    "streaming-iterables": "^6.0.0"
  },
  "dependencies": {
    "abortable-iterator": "^3.0.1",
    "class-is": "^1.1.0",
    "debug": "^4.3.1",
    "err-code": "^3.0.1",
    "libp2p-utils": "^0.4.0",
    "mafmt": "^10.0.0",
    "multiaddr": "^10.0.0",
    "stream-to-it": "^0.2.2"
  },
